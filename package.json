  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production && integration-code/node_modules/webpack-cli/bin/cli.js --config integration-code/webpack.prod.js",
    "postbuild": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION",
    "build:integration": "integration-code/node_modules/webpack-cli/bin/cli.js --config integration-code/webpack.prod.js",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.2",
    "@paypal/paypal-js": "^4.2.1",
    "@stripe/stripe-js": "^1.22.0",
    "@vee-validate/i18n": "^4.5.7",
    "@vee-validate/rules": "^4.5.7",
    "@vueuse/core": "^7.3.0",
    "@vueuse/shared": "^7.3.0",
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "babel-eslint": "^10.1.0",
    "canvas-confetti": "^1.4.0",
    "core-js": "^3.6.5",
    "js-cookie": "^3.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tiny-emitter": "^2.1.0",
    "vee-validate": "^4.5.7",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vue3-webcomponent-wrapper": "^0.1.4",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "^9",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-remove-attribute": "^1.0.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.3",
    "postcss": "^7",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
